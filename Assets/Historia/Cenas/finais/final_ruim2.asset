      tipoAcao: 1
      posicao: {x: 400, y: 0}
    fx: {fileID: 0}
  - texto: "Como voc\xEA est\xE1? "
    personagem: {fileID: 11400000, guid: 3498a50f9b7d3994b8d40e148a56695d, type: 2}
    acoes: []
    fx: {fileID: 0}
  - texto: "Voc\xEA t\xE1 bem? "
    personagem: {fileID: 11400000, guid: bc171279a801ee340a35c29963ab424f, type: 2}
    acoes:
    - personagem: {fileID: 11400000, guid: bc171279a801ee340a35c29963ab424f, type: 2}
      spriteIndex: 4
      tipoAcao: 0
      posicao: {x: 0, y: 0}
    fx: {fileID: 0}
  - texto: 'Estou melhor...  '
    personagem: {fileID: 11400000, guid: 4f168e12a51ad0a4bb9b930e7a569428, type: 2}
    acoes:
    - personagem: {fileID: 11400000, guid: bc171279a801ee340a35c29963ab424f, type: 2}
