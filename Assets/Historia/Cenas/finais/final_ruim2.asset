      spriteIndex: 1
      tipoAcao: 0
      posicao: {x: 0, y: 0}
    fx: {fileID: 0}
  - texto: "Mas, sendo bem sincero\u2026  "
    personagem: {fileID: 11400000, guid: 4f168e12a51ad0a4bb9b930e7a569428, type: 2}
    acoes: []
    fx: {fileID: 0}
  - texto: "Eu acho que mereci\u2026  "
    personagem: {fileID: 11400000, guid: 4f168e12a51ad0a4bb9b930e7a569428, type: 2}
    acoes: []
    fx: {fileID: 0}
  - texto: 'Cometi muitos erros com ela e com outras pessoas ao meu redor...  '
    personagem: {fileID: 11400000, guid: 4f168e12a51ad0a4bb9b930e7a569428, type: 2}
    acoes: []
    fx: {fileID: 0}
  - texto: "Inclusive com voc\xEA, Miguel...  "
    personagem: {fileID: 11400000, guid: 4f168e12a51ad0a4bb9b930e7a569428, type: 2}
    acoes: []
    fx: {fileID: 0}
  - texto: "Quero pedir desculpa a voc\xEAs, Player e Miguel\u2026  "
    personagem: {fileID: 11400000, guid: 4f168e12a51ad0a4bb9b930e7a569428, type: 2}
    acoes: []
    fx: {fileID: 0}
  - texto: 'O que eu fiz foi muito errado... '
    personagem: {fileID: 11400000, guid: 4f168e12a51ad0a4bb9b930e7a569428, type: 2}
    acoes: []
    fx: {fileID: 0}
